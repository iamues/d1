<%= simple_form_for(@studio) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :owner %>
    <%= f.input :established %>
    <%= f.input :fields %>
    <%= f.input :clients %>
    <%= f.association :user %>
    <%= f.input :freelance %>
    <%= f.input :size %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
